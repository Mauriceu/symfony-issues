<div {{ attributes }}>

    <h2>Test</h2>
    <label for="fromDate">From:</label>
    <input
        id="fromDate"
        type="date"
        value="{{ from.format('Y-m-d') }}"
        data-loading="addAttribute(disabled)"
        data-model="from"
        min="2024-01-01"
        max="2025-01-01"
    />

    <p>
        Current value: {{ from.format('Y-m-d') }}
    </p>
</div>